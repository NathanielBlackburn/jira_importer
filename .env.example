# Jira credentials only (required for import; not used in dry-run)
# Put jira.base_url in config.yml, not here.
# Basic Auth: <email>:<password> is sent as Base64 (supported by older Jira Server APIs)
JIRA_EMAIL=your-email@example.com
JIRA_PASSWORD=your-password

# Optional: path to config file (overrides --config default)
# CONFIG_PATH=config.yml

# Optional: skip SSL verification when fetching CSV from URLs (e.g. Google Sheets behind VPN). Insecure.
# CSV_SKIP_SSL_VERIFY=1
